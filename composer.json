{
	"name": "wikibase/query",
	"type": "mediawiki-extension",
	"description": "Extension to Wikibase Repo that adds the ability to define and execute queries",
	"keywords": [
		"Wikibase",
		"WikibaseQuery",
		"query",
		"Wikidata"
	],
	"homepage": "https://www.mediawiki.org/wiki/Extension:WikibaseQuery",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://bugzilla.wikimedia.org/",
		"irc": "irc://irc.freenode.net/wikidata",
		"forum": "https://www.mediawiki.org/wiki/Extension_talk:WikibaseQuery",
		"wiki": "https://www.mediawiki.org/wiki/Extension:WikibaseQuery"
	},
